<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="product">

	<select id="selectList">
		
		FROM WINE W
		WHERE W.STATUS = 'Y'
	</select>
	
	<insert id="enrollWine">
		
	</insert>
	
	<update id="deleteWine">
		UPDATE WINE
		SET STATUS = 'N'
	</update>

</mapper>